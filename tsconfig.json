{
  "compilerOptions": {
      "target": "es5",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": false,
      "sourceMap": false,
      // "module": "amd",
      "allowJs": false,  
      "checkJs": false,
      "declaration": true, 
      // "outFile": "./dist/1.js", 
      // "outDir": "./dist",    
      "rootDir": "./",  
      "strict": true,
      // "module": "commonjs",
      "noImplicitAny": true, 
      "alwaysStrict": true, 
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "strictNullChecks": true,
      
  },
  "include": ["./src/namespace.ts"],
}